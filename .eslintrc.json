{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "formValidator": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
